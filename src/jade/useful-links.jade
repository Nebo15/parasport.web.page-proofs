									i.icon.icon-arrow-link
								.links__item-title Paralympic Games for London 2012
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-link
								.links__item-title Cerebral Palsy International Sport and Recreation Association
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-link
								.links__item-title IBSA: International Blind Sports Association
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-link
								.links__item-title -FID INAS: International Sports Federation for Persons with Intellectual Disability
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-link
								.links__item-title IWASF: International Wheelchair and Amputee Sports Federation
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-link
								.links__item-title Disabled Peoples' International
					.links__wrap
						h1.links__h1.h1 Resources
						.links__list
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-download
								.links__item-title International Paralympic Committee Brochure (PDF 1.3 MB)
							a.links__item(href="#")
								.links__item-arrow
									i.icon.icon-arrow-download
