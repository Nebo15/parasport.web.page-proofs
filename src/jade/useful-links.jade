	footer.footer
		.center.footer__in
			.footer__logo
				a(href="index.html")
					img(src="img/logo-white.svg", width="94", alt="")
			.footer__menu
				.footer__link
					a(href="#") Cookies Policy
				.footer__link
					a(href="#") Terms & Conditions
				.footer__link
					a(href="#") Sitemap
				.footer__link
