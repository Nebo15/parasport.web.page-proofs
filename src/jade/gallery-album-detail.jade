	//- header
	.header
		.header__in
			.logo.logo_sm
				a(href="index.html")
					img(src="img/logo-sm.svg", width="34", alt="")
			button.humb.js-humb
				span
			.lang
				.lang__selected.js-lang-status
					span EN
					img(src="img/unionjack.png", alt="")
				ul.js-lang-list
					li.lang__item
						button(type="button").lang__btn.js-lang-btn.active
							span EN
							img(src="img/unionjack.png", alt="")
					li.lang__item
						button(type="button").lang__btn.js-lang-btn
							span RU
							img(src="img/Flag_of_Russia.png", alt="")
			.header__content.js-mob-menu			
				nav.nav
					ul.menu
						li.menu__link
							a(href="#").menu__about.js-dropdown-btn
								span ABOUT US
								.plus
							.dropdown.js-dropdown
