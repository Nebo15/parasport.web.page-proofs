								.plus
							.dropdown.js-dropdown
								ul
									li
										a(href="#") Paralympic sponsorship
									li
										a(href="#") Paralympic movement
									li
										a(href="#") Training
									li
										a(href="#") Helpful links and resources	
						li.menu__link
							a(href="#").menu__heroes
								span our heroes	
						li.menu__link
							a(href="#").menu__partners
								span partners
						li.menu__link
							a(href="#").menu__media.js-dropdown-btn
								span media & events
								.plus
							.dropdown.js-dropdown
								ul
									li
										a(href="#") News
									li
										a(href="#") We are in the news
									li
										a(href="#") Gallery
									li
										a(href="#") Events
				a(href="#").support-link
					span support us

